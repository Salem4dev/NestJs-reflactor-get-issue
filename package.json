  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=development SYNCHRONIZE=true nest start --watch",
    "start:debug": "NODE_ENV=development SYNCHRONIZE=true nest start --debug --watch",
    "start:prod": "NODE_ENV=production SYNCHRONIZE=false node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test SYNCHRONIZE=true jest --watch",
    "test:cov": "NODE_ENV=test SYNCHRONIZE=true jest --coverage",
    "test:debug": "NODE_ENV=test SYNCHRONIZE=true node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test SYNCHRONIZE=true jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@casl/ability": "^5.4.3",
    "@hapi/joi": "^17.1.1",
    "@nestjs-query/core": "^0.30.0",
